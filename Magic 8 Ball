let number = Math.floor(Math.random() * 5); 


switch (number) {
  case 1: 
    console.log("Yes!"); 
    break
  case 2: 
    console.log("No"); 
    break
  case 3:
    console.log("Ask again later"); 
    break 
  case 4: 
    console.log("Definitely"); 
    break 
  case 5: 
    console.log("Maybe"); 
    break 
  default: 
    console.log("Error!"); 
    break 

}
